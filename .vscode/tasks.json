{
  "version": "2.0.0",
  "tasks": [
    {
      "label": "Build WebApp",
      "command": "dotnet",
      "type": "process",
      "group": "build",
      "args": [
        "build",
        "${workspaceFolder}/WebApp/Olo42.SAROM.WebApp.csproj",
        "/property:GenerateFullPaths=true",
        "/consoleloggerparameters:NoSummary"
      ],
      "problemMatcher": "$msCompile"
    },
    {
      "label": "Publish WebApp",
      "command": "dotnet",
      "type": "process",
      "args": [
        "publish",
        "${workspaceFolder}/WebApp/Olo42.SAROM.WebApp.csproj",
        "/property:GenerateFullPaths=true",
        "/consoleloggerparameters:NoSummary"
      ],
      "problemMatcher": "$msCompile"
    },
    {
      "label": "Test WebApp",
      "command": "dotnet",
      "type": "process",
      "group": "test",
      "args": [
